<style>
    <tag name="arraygrid" variant="default">
        <set attribute="scrollanimationtype" value="fog(#000000,4)"/>
    </tag>
    <tag name="calendarfield" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="colline" variant="default">
        <set attribute="background" value="#C0C0C0"/>
        <set attribute="height" value="10"/>
        <set attribute="width" value="1"/>
    </tag>
    <tag name="colorfield" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="combofield" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="field" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="foldablepane" variant="default">
        <set attribute="bgpaint" value="rectangle(0,100%-3,100%,3,#80808020);rectangle(0,15,3,100%,#80808020);rectangle(100%-3,15,3,100%,#80808020);rectangle(0,0,100%,15,#80808080,#80808020,vertical);rectangle(3,15,100%-6,5,#80808020,#80808000,vertical)"/>
        <set attribute="innerbgpaint" value="rectangle(0,0,100%,100%,#FFFFFF40)"/>
        <set attribute="font" value="size:11;weight:bold"/>
        <set attribute="innerpadding" value="10"/>
        <set attribute="padding" value="3"/>
        <set attribute="image" value="/eclntjsfserver/images/foldablepanedefaulticon.png"/>
        <set attribute="curtaincolor" value="#80808050"/>
    </tag>
    <tag name="foldableverticalpane" variant="default">
        <set attribute="bgpaint" value="rectangle(15,0,100%,3,#80808020);rectangle(15,100%-3,100%,3,#80808020);rectangle(100%-3,3,100%,100%-3,#80808020);rectangle(0,0,15,100%,#80808080,#80808020,horizontal);rectangle(15,3,20,100%-6,#80808020,#80808000,horizontal)"/>
        <set attribute="innerbgpaint" value="rectangle(0,0,100%,100%,#FFFFFF40)"/>
        <set attribute="font" value="size:11;weight:bold"/>
        <set attribute="innerpadding" value="10"/>
        <set attribute="padding" value="3"/>
        <set attribute="image" value="/eclntjsfserver/images/foldableverticalpanedefaulticon.png"/>
        <set attribute="curtaincolor" value="#80808050"/>
    </tag>
    <tag name="foldablepopupinclude" variant="default">
        <set attribute="innerpadding" value="0"/>
        <set attribute="popupwidth" value="600"/>
        <set attribute="popupheight" value="400"/>
    </tag>
    <tag name="filechooser" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="fileupload" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="fixgrid" variant="default">
        <set attribute="scrollanimationtype" value="fog(#000000,4)"/>
    </tag>
    <tag name="formattedfield" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="gridcol" variant="default">
        <set attribute="background" value="#C0C0C0"/>
    </tag>
    <tag name="gridheaderlabel" variant="default">
        <set attribute="background" value="#C0C0C0"/>
    </tag>
    <tag name="modalpopup" variant="default">
        <set attribute="bgpaint" value="rectangle(0,0,100%,100%,#F0F0F0,#909090,vertical)"/>
    </tag>
    <tag name="modelesspopup" variant="default">
        <set attribute="bgpaint" value="rectangle(0,0,100%,100%,#F0F0F0,#D0D0D0,vertical)"/>
    </tag>
    <tag name="password" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="rowbodypane" variant="default">
        <set attribute="background" value="#FFFFFFA0"/>
        <set attribute="padding" value="20"/>
    </tag>
    <tag name="rowheader" variant="default">
        <set attribute="background" value="#F0F0F0C0"/>
        <set attribute="border" value="bottom:1;color:#C0C0C0"/>
        <set attribute="padding" value="3"/>
    </tag>
    <tag name="rowline" variant="default">
        <set attribute="background" value="#C0C0C0"/>
        <set attribute="height" value="1"/>
    </tag>
    <tag name="rowtitlebar" variant="default">
        <set attribute="border" value="top:0;left:0;right:0;bottom:0;color:#909090"/>
        <set attribute="bgpaint" value="rectangle(0,0,100%,100%,#000000A0,#00000020,vertical)"/>
        <set attribute="font" value="size:15;weight:bold"/>
        <set attribute="foreground" value="#F0F0F0"/>
        <set attribute="padding" value="3"/>
    </tag>
    <tag name="rowstatusbar" variant="default">
        <set attribute="background" value="#F0F0F0"/>
        <set attribute="border" value="left:0;top:1;right:0;bottom:0;color:#C0C0C0"/>
        <set attribute="padding" value="3"/>
        <set attribute="font" value="#{eclntdefscr.statusbar.font}"/>
        <set attribute="image" value="#{eclntdefscr.statusbar.image}"/>
        <set attribute="text" value="#{eclntdefscr.statusbar.text}"/>       
        <set attribute="bgpaint" value="#{eclntdefscr.statusbar.bgpaint}"/>       
    </tag>
    <tag name="shadowedpane" variant="default">
        <set attribute="background" value="#00000020"/>
        <set attribute="shadowdistance" value="5"/>
        <set attribute="shadowbackground" value="#0000000b"/>
    </tag>
    <tag name="splitpane" variant="default">
        <set attribute="dividersize" value="4"/>
        <set attribute="dividercolor" value="#D0D0D0"/>
    </tag>
    <tag name="spinner" variant="default">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="tabbedpane" variant="default">
        <set attribute="tabshadingbackground1" value="#FFFFFFC0"/>
        <set attribute="tabshadingbackground2" value="#FFFFFF00"/>
    </tag>

    <!-- explicit transparent usage of text pane e.g. in popups -->
    <tag name="textpane" variant="TRANSPARENT">
    </tag>
    
    <!--  WORKPAGE VARIANTS -->
    <tag name="rowworkplacefunctiontree" variant="default">
        <set attribute="foreground" value="#FFFFFF"/>
        <set attribute="scrollanimationtype" value="fog(#FFFFFF,6)"/>
    </tag>
    <tag name="tabbedline" variant="WP_WORKPAGESELECTOR">
        <set attribute="bgpaintdefault" value="rectangle(0,0,100%,100%,#80808080,#80808020,horizontal)"/>
        <set attribute="bgpaintrollover" value="rectangle(0,0,100%,100%,#C0808080,#C0808020,horizontal)"/>
        <set attribute="bgpaintselected" value="rectangle(0,0,100%,100%,#C0808080,#C0808020,horizontal)"/>
        <set attribute="font" value="size:10"/>
        <set attribute="foregrounddefault" value="#808080"/>
        <set attribute="foregroundselected" value="#FFFFFF"/>
        <set attribute="rowalignmenty" value="bottom"/>
        <set attribute="indentwidth" value="0"/>
    </tag>
    <tag name="button" variant="WP_ISOLATEDWORKPAGE">
        <set attribute="contentareafilled" value="false"/>
        <set attribute="foreground" value="#f0f0f0"/>
    </tag>
    <tag name="rowheader" variant="WP_ISOLATEDWORKPAGE">
        <set attribute="bgpaint" value="rectangle(0,0,100%,100%,#00000050,#00000020,vertical)"/>
        <set attribute="border" value="bottom:1;color:#C0C0C0"/>
        <set attribute="padding" value="1"/>
    </tag>
    
    <!--  IDVALUESELECTION VARIANTS -->
    <tag name="pane" variant="IDVALUESELECTION">
        <set attribute="border" value="#808080"/>
    </tag>
    <tag name="fixgrid" variant="IDVALUESELECTION">
        <set attribute="background" value="#E0E0E0"/>
        <set attribute="borderheight" value="1"/>
        <set attribute="borderwidth" value="1"/>
    </tag>
    <tag name="label" variant="IDVALUESELECTION">
        <set attribute="background" value="#F0F0F0"/>
    </tag>
    <tag name="rowheader" variant="IDVALUESELECTION">
        <set attribute="background" value="#E0E0E0"/>
        <set attribute="border" value="top:1;color:#C0C0C0"/>
        <set attribute="padding" value="3"/>
    </tag>
    <tag name="rowbodypane" variant="IDVALUESELECTION">
        <set attribute="background" value="#FFFFFF"/>
    </tag>
    <tag name="textpane" variant="IDVALUESELECTION">
        <set attribute="background" value="#FFFFF0"/>
    </tag>
    
    <!--  TABBED LINE STYLE VARIANTS -->
	<tag name="t:tabbedline" variant="cctl01" >
	  <set attribute="bgpaintdefault" value="rectangle(0,0,100%,100%,#d0d0d0,#f0f0f0,vertical)" />
	  <set attribute="bgpaintdefaultfirst" value="roundedrectangle(0,0,50,100%,25,25,#d0d0d0,#f0f0f0,vertical);rectangle(25,0,100%,100%,#d0d0d0,#f0f0f0,vertical)" />
	  <set attribute="bgpaintdefaultlast" value="roundedrectangle(100%-50,0,50,100%,25,25,#d0d0d0,#f0f0f0,vertical);rectangle(0,0,100%-30,100%,#d0d0d0,#f0f0f0,vertical)" />
	  <set attribute="bgpaintrollover" value="rectangle(0,0,100%,100%,#e9b475,#fefdd3,vertical)" />
	  <set attribute="bgpaintrolloverfirst" value="roundedrectangle(0,0,50,100%,25,25,#e9b475,#fefdd3,vertical);rectangle(25,0,100%,100%,#e9b475,#fefdd3,vertical)" />
	  <set attribute="bgpaintrolloverlast" value="roundedrectangle(100%-50,0,50,100%,25,25,#e9b475,#fefdd3,vertical);rectangle(0,0,100%-30,100%,#e9b475,#fefdd3,vertical)" />
	  <set attribute="bgpaintselected" value="rectangle(0,0,100%,100%,#75b4e9,#d3fdfe,vertical)" />
	  <set attribute="bgpaintselectedfirst" value="roundedrectangle(0,0,50,100%,25,25,#75b4e9,#d3fdfe,vertical);rectangle(25,0,100%,100%,#75b4e9,#d3fdfe,vertical)" />
	  <set attribute="bgpaintselectedlast" value="roundedrectangle(100%-50,0,50,100%,25,25,#75b4e9,#d3fdfe,vertical);rectangle(0,0,100%-30,100%,#75b4e9,#d3fdfe,vertical)" />
	  <set attribute="fontselected" value="weight:bold" />
	  <set attribute="foregrounddefault" value="#404040" />
	  <set attribute="foregroundselected" value="#000000" />
	  <set attribute="menuiconvisible" value="false" />
	</tag>
	<tag name="t:tabbedline" variant="cctl02" >
	  <set attribute="bgpaintdefault" value="rectangle(0,0,100%,100%,#d0d0d0,#f0f0f0,vertical)" />
	  <set attribute="bgpaintdefaultfirst" value="roundedrectangle(0,0,50,100%,25,25,#d0d0d0,#f0f0f0,vertical);rectangle(25,0,100%,100%,#d0d0d0,#f0f0f0,vertical)" />
	  <set attribute="bgpaintdefaultlast" value="roundedrectangle(100%-50,0,50,100%,25,25,#d0d0d0,#f0f0f0,vertical);rectangle(0,0,100%-30,100%,#d0d0d0,#f0f0f0,vertical)" />
	  <set attribute="bgpaintrollover" value="rectangle(0,0,100%,100%,#e9b475,#fefdd3,vertical)" />
	  <set attribute="bgpaintrolloverfirst" value="roundedrectangle(0,0,50,100%,25,25,#e9b475,#fefdd3,vertical);rectangle(25,0,100%,100%,#e9b475,#fefdd3,vertical)" />
	  <set attribute="bgpaintrolloverlast" value="roundedrectangle(100%-50,0,50,100%,25,25,#e9b475,#fefdd3,vertical);rectangle(0,0,100%-30,100%,#e9b475,#fefdd3,vertical)" />
	  <set attribute="bgpaintselected" value="rectangle(0,0,100%,100%,#804040,#e04040,vertical)" />
	  <set attribute="bgpaintselectedfirst" value="roundedrectangle(0,0,50,100%,25,25,#804040,#e04040,vertical);rectangle(25,0,100%,100%,#804040,#e04040,vertical)" />
	  <set attribute="bgpaintselectedlast" value="roundedrectangle(100%-50,0,50,100%,25,25,#804040,#e04040,vertical);rectangle(0,0,100%-30,100%,#804040,#e04040,vertical)" />
	  <set attribute="fontselected" value="weight:bold" />
	  <set attribute="foregrounddefault" value="#404040" />
	  <set attribute="foregroundselected" value="#F0F0F0" />
	  <set attribute="menuiconvisible" value="false" />
	</tag>
	<tag name="t:tabbedline" variant="cctl03" >
	  <set attribute="bgpaintdefault" value="image(0,0,/eclntjsfserver/images/tabbedline/tl1_defleft.png&amp;mirrorrtl,lefttop);image(100%,0,/eclntjsfserver/images/tabbedline/tl1_defright.png&amp;mirrorrtl,righttop);image(5,0,100%-22,25,/eclntjsfserver/images/tabbedline/tl1_defmid.png&amp;mirrorrtl,lefttop)" />
	  <set attribute="bgpaintrollover" value="image(0,0,/eclntjsfserver/images/tabbedline/tl1_selleft.png&amp;mirrorrtl,lefttop);image(100%,0,/eclntjsfserver/images/tabbedline/tl1_selright.png&amp;mirrorrtl,righttop);image(5,0,100%-22,25,/eclntjsfserver/images/tabbedline/tl1_selmid.png&amp;mirrorrtl,lefttop)" />
	  <set attribute="bgpaintselected" value="image(0,0,/eclntjsfserver/images/tabbedline/tl1_selleft.png&amp;mirrorrtl,lefttop);image(100%,0,/eclntjsfserver/images/tabbedline/tl1_selright.png&amp;mirrorrtl,righttop);image(5,0,100%-22,25,/eclntjsfserver/images/tabbedline/tl1_selmid.png&amp;mirrorrtl,lefttop)" />
	  <set attribute="border" value="bottom:1;color:#00000030" />
	  <set attribute="font" value="weight:bold" />
	  <set attribute="fontselected" value="weight:bold" />
	  <set attribute="tabheight" value="25" />
	  <set attribute="taboverlap" value="7" />
	</tag>
	<tag name="t:tabbedline" variant="cctl04" >
	  <set attribute="bgpaintdefault" value="roundedborder(0,0,100%,100,10,10,#00000030,1)" />
	  <set attribute="bgpaintrollover" value="roundedborder(0,0,100%,100,10,10,#FF000030,1)" />
	  <set attribute="bgpaintselected" value="roundedrectangle(0,0,100%,100,10,10,#FFFFFF);roundedborder(0,0,100%,100,10,10,#00000050,1)" />
	  <set attribute="border" value="bottom:1;color:#00000030" />
	  <set attribute="font" value="weight:bold" />
	  <set attribute="fontselected" value="weight:bold" />
	  <set attribute="foregrounddefault" value="#00000060" />
	  <set attribute="foregroundselected" value="#000000" />
	</tag>
	
	<tag name="t:tabbedpane" variant="topmenu" >
	  <set attribute="background" value="#FFFFFF60" />
	  <set attribute="borderdarkshadowcolor" value="#00000030" />
	  <set attribute="borderlightcolor" value="#00000020" />
	  <set attribute="bordershadowcolor" value="#00000000" />
	  <set attribute="font" value="weight:bold" />
	  <set attribute="onetablineonly" value="true" />
	  <set attribute="onlydrawselectedtab" value="true" />
	  <set attribute="tabindent" value="30" />
	  <set attribute="tabstyle" value="classic" />
	  <set attribute="transparent" value="true" />
	</tag>
	<tag name="t:tabbedpanetab" variant="topmenu" >
	  <set attribute="padding" value="10" />
	</tag>
	
</style>
